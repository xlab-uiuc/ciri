DEFAULT_EXCEPTION_REPORTER='django.views.debug.ExceptionReporter'

USE_I18N=False

FILE_CHARSET=utf-8

LANGUAGE_COOKIE_HTTPONLY=True

MONTH_DAY_FORMAT='F j'

